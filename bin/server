#!/usr/bin/env php
<?php

use Sws\Application;

define('BASE_PATH', dirname(__DIR__) );

session_start();

require dirname(__DIR__) . '/boot/web.php';

$mgr = $di->get('app');
//$mgr = new \Sws\server\WebSocketServer($config);
//$mgr = new \inhere\server\BoxServer($config);

$mgr->attachListener('port2', new \inhere\server\portListeners\UdpListener([
    'host' => '0.0.0.0',
    'port' => '9762',
]));

/**
 * do run
 * @var Application $mgr
 */
$mgr->run();